All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
<ul class=job>
  <%= job.title %> at <%= job.company.name %>
  <%= link_to "Edit #{job.title} Job", edit_company_job_path(@company, job) %>
  <%= link_to "Delete", company_job_path( @company, job), method: :delete %></ul>
<% end %>
